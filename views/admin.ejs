        <script>
            function init() {
                var mapMinZoom = 2;
                var mapMaxZoom = 3;
                var map = L.map('map', {
                    maxZoom: mapMaxZoom,
                    minZoom: mapMinZoom,
                    zoomControl:false,                    
                    crs: L.CRS.Simple
                }).setView([0, 0], mapMinZoom);
                map.dragging.disable();
                map.touchZoom.disable();
                map.doubleClickZoom.disable();
                map.scrollWheelZoom.disable();
                if(map.tap) map.tap.disable();
                var mapBounds = new L.LatLngBounds(
                        map.unproject([0, 750], mapMaxZoom),
                        map.unproject([1280, 0], mapMaxZoom));

                map.fitBounds(mapBounds);
                L.tileLayer('/img/hockey/{z}/{x}/{y}.png', {
                    minZoom: mapMinZoom, maxZoom: mapMaxZoom,
                    bounds: mapBounds,
                    attribution: 'Team Raisins',
                    noWrap: true
                }).addTo(map);

                var heat = L.heatLayer(quakePoints, {
                    radius: 13,
                    blur: 20
                }).addTo(map);
            }
        </script>

